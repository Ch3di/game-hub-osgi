package sokobanCore;

import java.awt.Image;

import javax.swing.ImageIcon;

public class Area extends Actor {

    public Area(int x, int y) {
        super(x, y);
        
        initArea();
    }
    b
    private void initArea() {

        ImageIcon iicon = new ImageIcon("area.png");
        Image image = iicon.getImage();
        setImage(image);
    }
}
